<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Chronicity.io </title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-side-navbar/source/assets/stylesheets/navbar-fixed-side.css" />
    <link rel="stylesheet" href="~/lib/vis/dist/vis-timeline-graph2d.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />


</head>
<body>

    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/collapse.js"></script>
    <script src="~/lib/bootstrap/js/modal.js"></script>
    <script src="~/lib/vis/dist/vis.js"></script>
    <script src="~/lib/ace-builds/src-min-noconflict/ace.js"></script>
    <script src="~/lib/handlebars/handlebars.js"></script>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-111977441-2"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-111977441-2');
    </script>

    <script async defer src="https://buttons.github.io/buttons.js"></script>


    @RenderBody()
    @RenderSection("Scripts", required: false)
</body>
</html>
